{
    "name": "@vertigis/workflow-sdk",
    "version": "0.0.0-semantically-released",
    "description": "The SDK for extending VertiGIS Studio Workflow",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vertigis/vertigis-workflow-sdk.git"
    },
    "author": "VertiGIS",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vertigis/vertigis-workflow-sdk/issues"
    },
    "homepage": "https://github.com/vertigis/vertigis-workflow-sdk#readme",
    "files": [
        "bin",
        "config",
        "lib",
        "scripts",
        "template"
    ],
    "type": "module",
    "engines": {
        "node": ">=18",
        "npm": ">=8"
    },
    "bin": {
        "vertigis-workflow-sdk": "bin/vertigis-workflow-sdk.js"
    },
    "scripts": {
        "create": "cross-env SDK_LOCAL_DEV=true node bin/vertigis-workflow-sdk.js create",
        "test": "npm run test:unit && npm run test:e2e",
        "test:e2e": "node ./test/e2e/index.js",
        "test:unit": "jest",
        "test:unit:watch": "npm run test:unit -- --watch",
        "prettier": "prettier --write \"**/*.json\" \"**/*.js\""
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "~7.4.0",
        "@typescript-eslint/parser": "~7.4.0",
        "chalk": "~5.3.0",
        "clean-webpack-plugin": "~4.0.0",
        "css-loader": "~7.1.0",
        "eslint": "~8.57.0",
        "eslint-config-prettier": "~9.1.0",
        "eslint-plugin-react": "~7.36.0",
        "eslint-plugin-react-hooks": "~4.6.0",
        "fs-extra": "~11.2.0",
        "inquirer": "~7.3.3",
        "postcss": "~8.4.38",
        "postcss-loader": "~8.1.1",
        "postcss-preset-env": "~9.5.2",
        "style-loader": "~4.0.0",
        "ts-loader": "~9.5.1",
        "ts-morph": "~9.1.0",
        "url-loader": "~4.1.1",
        "webpack": "~5.101.3",
        "webpack-dev-server": "~5.2.2",
        "yargs": "~17.7.2"
    },
    "devDependencies": {
        "@babel/core": "^7.26.8",
        "@babel/preset-env": "^7.26.8",
        "@babel/preset-typescript": "7.26.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.30",
        "@vertigis/workflow": "^5.45.1",
        "conventional-changelog-conventionalcommits": "^7.0.2",
        "cross-env": "^7.0.3",
        "execa": "^9.0.0",
        "husky": "^4.3.6",
        "jest": "^29.7.0",
        "node-fetch": "^2.6.7",
        "p-retry": "^4.6.2",
        "prettier": "^3.2.5",
        "pretty-quick": "^4.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "typescript": "^5.4.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
